// some single line comment
int meh(private bytearr[10] b) {
  private int a = 5;
  private int bb = 1;
  b[0] = 0b10101; // 1 + 4 + 16 = 21
  b[a] = bb;
  if(a > 1) {
    private int two = 2;
    private int zero = 0;
    b[zero] = two;
  } else {
    a = 1;
  }
  for(i=0 to 1) {
    private int i1 = i;
    b[i1] = i1;
  }
  return 0x1;
}

