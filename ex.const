int meh(bytearr[1] b) {
  b[0] = 0b10101; // 1 + 4 + 16 = 21
  int a = 5;
  if(a > 1) {
    b[0] = 2;
  } else {
    a = 1;
  }
  for(i=0 to 1) {
    b[i] = i;
  }
  return 0x1;
}

