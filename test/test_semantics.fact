secret int32 prec(secret int32 a) {
  if (a == 1) {
    return 2 + 3 * 4;
  } else {
    if (a == 2) {
      return (2 + 3) * 4;
    } else {
      if (a == 3) {
        return -2 + 3;
      } else {
        return -1;
      }
    }
  }
  // TODO: This return -11 is needed to compile, otherwise itll insert a void
  //       ret and fail to type check at llvm level.
  return -11;
}

secret int32 opassign() {
  secret mut int32 a = 1;
  a += 5; // 6
  a -= 2; // 4
  a *= 3; // 12
  a <<= 3; // 12 * 8
  a >>= 2; // 24 == 0b11000
  a &= 0b101011; // 0b1000
  a |= 0b0101; // 0b1101
  a ^= 0b0110; // 0b1011 == 11
  return a;
}

