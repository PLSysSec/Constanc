secret int32 nestedIf(secret int32 cond) {
  if (10 > cond) {
    if (5 > cond) {
      return 1;
    } else {
      return 2;
    }
  } else {
    if (15 > cond) {
      return 3;
    } else {
      return 4;
    }
  }
  // TODO: This return -11 is needed to compile, otherwise itll insert a void
  //       ret and fail to type check at llvm level.
  return -1;
}

